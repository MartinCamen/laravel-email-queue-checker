{
    "name": "adaptivemedia/laravel-email-queue-checker",
    "description": "Will check that your applications email queue is working properly",
    "keywords": [
        "adaptive media",
        "email-queue-checker"
    ],
    "homepage": "https://github.com/adaptivemedia/laravel-email-queue-checker",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Eliasson",
            "email": "victor@adaptivemedia.se",
            "homepage": "https://adaptivemedia.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7",
        "illuminate/console": "~5.2|^6.0",
        "illuminate/support": "~5.2|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Adaptivemedia\\EmailQueueChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adaptivemedia\\EmailQueueChecker\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adaptivemedia\\EmailQueueChecker\\EmailQueueCheckerServiceProvider"
            ]
        }
    }
}
